                <script>
                        function validate() {
                         var fn=document.getElementById('firstName').value;
                         var ln=document.getElementById('lastName').value;
                         var em=document.getElementById('email').value ;
                         var pn=document.getElementById('bd').value;
                          var comments=document.getElementById('comments').value; 
                        
                         if(fn.length==0){
                         alert("Please enter First Name");
                         return false;
                         }
                         if(ln.length==0){
                        alert("please enter your Last Name again");
                        return false;
                        }
                        
                        if(em.length==0){
                        alert("please enter your Email again");
                        return false;
                        }
                         var pos=em.indexOf('@deakin.edu.au');
                                        //alert(pos);
                                        if (pos==-1){
                                        alert('provide deakin email');
                                         return false;
                                        }
                        
                       
                        
                        if(bd.length==0){
                        alert("please enter your birthday ");
                        return false;
                        }
                        
                               
                        return true;
                        }
                        
                        
        
